package fr.afpa.math;
import java.math.BigDecimal;

import javax.swing.JOptionPane;


public class euroToDollar {

	final static BigDecimal TAUX_EURO_DOLLAR= new BigDecimal("1.18674");
	static BigDecimal  euro;
	static BigDecimal  dollar;
	
	
	public static BigDecimal eurosToDollars(BigDecimal euro) {
	
		dollar= euro.multiply(TAUX_EURO_DOLLAR) ;	
		
		return dollar;       
	}
}